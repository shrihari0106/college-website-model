  <!-- WHY CHOOSE US -->
  <section class="whyus">
    <div class="container-fluid px-2">
      <div class="row">
        <div class="col-12 text-center p-3">
          <h3
            style="color: #004d80; font-family:'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif">
            Lots to choose from, yet why choose us?
          </h3>
        </div>

